package com.company;
import java.util.Scanner;
import static java.lang.Math.*;
public class main
{
    public static void main(String[] args)
    {
        Dog pies = new Dog();
        Cat kot = new Cat();
        
        kot.setColor("blekitny")
        pies.setColor("szmaragdowy");
        kot.setName("hemoglobina")
        pies.setName("przediwiatr");
       
        System.out.println(pies.makeSound());
        System.out.println(kot.makeSound());
    }
}
